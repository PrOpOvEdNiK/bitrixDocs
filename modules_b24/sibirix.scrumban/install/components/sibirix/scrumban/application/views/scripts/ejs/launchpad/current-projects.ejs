<script type="text/ejs" id="launchpadCurrentProjects">
    <div class="current-projects">
        <div class="projects-header">
            <div class="projects-title">
                <span class="top-title"><?= $this->t('LAUNCHPAD_PROJECTS_MY'); ?></span>
                <small><?= $this->t('LAUNCHPAD_PROJECTS_ACTIVITY'); ?></small>
            </div>
            <div class="notify-title js-notify-title">
                <span class="top-title"><?= $this->t('LAUNCHPAD_PROJECTS_NOTIFY'); ?></span>
                <small>what's going on</small>
            </div>
        </div>

        <div class="projects-list-wrapper">
            <div class="projects-list js-projects-list">
                <div class="projects-page js-projects-page">
                    <% for (var i = 0; i < this.projectList.length; i++) {
                        var project = this.projectList[i];
                        if (i && i % 5 == 0) { %>
                            </div>
                            <div class="projects-page js-projects-page">
                        <% } %>
                        <a href="<%= project.url %>" class="project <% if (parseInt(project.supportEnabled, 10) > 0) { %>support<% } %> js-project"
                            data-id="<%= project.groupId == 'empty' && project.siteId ? project.siteId : project.groupId %>">
                            <div class="image-wrapper"><img src="<%= project.image %>" width="48" height="48" alt=""></div>
                            <div class="name-wrapper"><%= project.name %></div>
                            <div class="clearfix"></div>
                        </a>
                    <% } %>
                </div>
            </div>
            <div class="notify-list-wrapper js-notify-list"></div>
            <div class="sprint-list-wrapper js-sprint-list-wrapper" style="display: none"></div>
        </div>
    </div>
</script>
